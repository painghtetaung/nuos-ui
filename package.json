{
  "name": "magick-ui",
  "version": "0.1.4",
  "description": "Generative UI primitives, renderer, design tokens, and schema for composing dynamic chat interfaces",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./tokens": {
      "types": "./dist/design-tokens/design-tokens.d.ts",
      "import": "./dist/design-tokens/design-tokens.js",
      "require": "./dist/design-tokens/design-tokens.cjs"
    },
    "./schema.json": "./schema/component-schema.json"
  },
  "files": [
    "dist",
    "schema",
    "src/design-tokens"
  ],
  "scripts": {
    "generate-schema": "tsx src/schema/generator.ts",
    "build": "pnpm generate-schema && tsup",
    "dev": "tsup --watch",
    "build-tokens": "tsx src/design-tokens/generator.ts",
    "playground": "vite --config playground/vite.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.511.0",
    "tailwind-merge": "^3.0.0",
    "zod": "3",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@types/node": "^25.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^10.2.10",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "generative-ui",
    "react",
    "chat",
    "ai",
    "components",
    "design-tokens"
  ],
  "license": "MIT"
}
