{
  "name": "magick-ui",
  "version": "0.1.0",
  "description": "Generative UI primitives, renderer, design tokens, and schema for composing dynamic chat interfaces",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js",
      "require": "./dist/primitives/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./tokens": {
      "types": "./dist/design-tokens/design-tokens.d.ts",
      "import": "./dist/design-tokens/design-tokens.js",
      "require": "./dist/design-tokens/design-tokens.cjs"
    },
    "./schema.json": "./schema/component-schema.json"
  },
  "files": [
    "dist",
    "schema",
    "src/design-tokens"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build-tokens": "tsx src/design-tokens/generator.ts"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.511.0",
    "tailwind-merge": "^3.0.0"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "generative-ui",
    "react",
    "chat",
    "ai",
    "components",
    "design-tokens"
  ],
  "license": "MIT"
}
