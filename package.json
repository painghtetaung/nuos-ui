{
  "name": "magick-ui",
  "version": "0.2.0",
  "description": "Generative UI primitives, renderer, design tokens, and schema for composing dynamic chat interfaces",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./tokens": {
      "types": "./dist/design-tokens/design-tokens.d.ts",
      "import": "./dist/design-tokens/design-tokens.js",
      "require": "./dist/design-tokens/design-tokens.cjs"
    },
    "./schema.json": "./schema/component-schema.json"
  },
  "files": [
    "dist",
    "schema",
    "src/design-tokens"
  ],
  "scripts": {
    "generate-schema": "tsx src/schema/generator.ts",
    "build": "pnpm generate-schema && tsup",
    "prepublishOnly": "pnpm build",
    "dev": "tsup --watch",
    "build-tokens": "tsx src/design-tokens/generator.ts",
    "playground": "vite --config playground/vite.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-hook-form": ">=7.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lodash": "^4.17.23",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.511.0",
    "magick-icons": "^0.1.270",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.8",
    "react-day-picker": "^9.13.2",
    "react-dropzone": "^15.0.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.0",
    "vaul": "^1.1.2",
    "zod": "3",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@tailwindcss/vite": "^4.2.0",
    "@types/lodash": "^4.17.24",
    "@types/node": "^25.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^10.2.10",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  },
  "keywords": [
    "generative-ui",
    "react",
    "chat",
    "ai",
    "components",
    "design-tokens"
  ],
  "license": "MIT"
}
